}
.text_with_overlay .left_block_desc li:before{
  content: '';
  width: 30px;
  height: 30px;
  background: url('{{ "blue-textured-circle.svg" | file_url }}') no-repeat;
  position: absolute;
  top: -6px;
  left: 0;
  z-index: -1;
