<section class="Section Section--spacingNormal founders_section" id="section-{{ section.id }}" data-section-id="{{ section.id }}">
  <div class="Container">
    <div class="founders">
      {%- if section.settings.subheading != blank or section.settings.title != blank -%}
        <header class="SectionHeader SectionHeader--center text_center animate show_up ad-1">
          {%- if section.settings.title != blank -%}
            <h2 class="SectionHeader__Heading Heading u-h1 sec_heading">{{ section.settings.title }}
            {% if section.settings.heading_image != blank %}
              <figure class="heading_img animate"><img src="{{ section.settings.heading_image | img_url: 'master'  }}" alt="" /></figure>
            {% endif %}
            </h2>
          {%- endif -%}
          {%- if section.settings.desc != blank -%}
            <p>{{ section.settings.desc }}</p>
          {%- endif -%}
        </header>
      {%- endif -%}
      <div>
        
      </div>
    </div>
  </div>
</section>



{% schema %}
  {
    "name": "Founders",
    "settings": [
      {
        "type": "text",
        "id": "title",
        "label": "Heading"
      },
      {
        "type": "text",
        "id": "sub_title",
        "label": "Sub Heading"
      }
    ],
    "blocks": [
      {
        "type": "Team",
        "name": "Team",
        "settings": [
          {
            "type": "image_picker",
            "id": "image",
            "label": "Image"
          },
          {
            "type": "textarea",
            "id": "desc",
            "label": "Description"
          }
        ]
      }
    ],
    "presets": [
      {
        "name": "Founders"
      }
    ]
  }
{% endschema %}

{% stylesheet %}
{% endstylesheet %}

