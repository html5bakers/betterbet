<div class="PageHeader_wrapper">
  <header class="PageHeader">
    <div class="SectionHeader SectionHeader--center Container">
      <h1 class="SectionHeader__Heading Heading u-h1 sec_heading animate ad-1">{{ page.title }}</h1>
    </div>
  </header>
</div>
<div class="PageContent Rte Container animate show_up ad-1-5">
  {{ page.content }}
</div>
{{  page.template_suffix   }} {{ template }}
{% if page.template_suffix  %}{% endif %}
{% comment %}
{% schema %}
{
  "name": "Page",
  "settings": [
    {
      "type": "checkbox",
      "id": "show_large",
      "label": "Large width",
      "default": false
    }
  ]
}
{% endschema %}
{% endcomment %}